/*
 * DEFAULT TERRA++ TREE COVER DATASETS
 */

[
    //whole world at max resolution
    {
        "dataset": {
            "urls": [
                "https://cloud.daporkchop.net/gis/treecover2000/13/${x}/${z}.tiff"
            ],
            "projection": {
                "scale": {
                    "delegate": {
                        "web_mercator": {
                            "zoom": 13
                        }
                    },
                    "x": 1.0,
                    "y": 1.0
                }
            },
            "resolution": 256,
            "blend": "CUBIC",
            "parse": {
                "divide": {
                    "delegate": {
                        "parse_tiff": {}
                    },
                    "value": 100.0
                }
            }
        },
        "zooms": 0,
        "bounds": {
            "minX": -180.0,
            "maxX": 180.0,
            "minZ": -60,
            "maxZ": 80
        },
        "priority": -100.0
    }
]
